{
    "name": "intracto/secretsanta",
    "license": "MIT",
    "type": "project",
    "description": "Secret santa is the #1 online gift exchange organizer.",
    "autoload": {
        "psr-4": {
            "Intracto\\": "src/Intracto/",
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/orm": "^2.5.11",
        "easycorp/easy-log-handler": "^1.0.2",
        "geoip2/geoip2": "^2.6",
        "google/apiclient": "^1.1",
        "jms/i18n-routing-bundle": "^3.0",
        "phpdocumentor/reflection-docblock": "^3.0|^4.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^4.0",
        "symfony/cache": "^3.3|^4.0",
        "symfony/console": "^4.0",
        "symfony/debug-bundle": "^3.3|^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.3|^4.0",
        "symfony/process": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/property-access": "^3.3|^4.0",
        "symfony/property-info": "^3.3|^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/serializer": "^3.3|^4.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/validator": "^4.0",
        "symfony/var-dumper": "^3.3|^4.0",
        "symfony/web-link": "^4.0",
        "symfony/web-server-bundle": "^3.3",
        "symfony/webpack-encore-pack": "*",
        "symfony/yaml": "^4.0",
        "twig/extensions": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "behat/behat": "^3.2",
        "behat/mink": "dev-master",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "se/selenium-server-standalone": "3.4.*",
        "lakion/mink-debug-extension": "^1.2",
        "webmozart/assert": "^1.1",
        "doctrine/data-fixtures": "^1.2",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "symfony/dotenv": "^4.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "travis-build": [
            "@auto-scripts"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "hard"
    }
}
